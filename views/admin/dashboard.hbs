                        {{#each doctors}}
                        <tr>
                            <td><img src="/images/{{this._id}}.jpg" alt=""></td>
                            <td>Dr. {{this.firstname}} {{this.lastname}}</td>
                            <td>{{this.gender}}</td>
                            <td>{{this.specialized}}</td>
                            <td>{{this.speciality}}</td>
