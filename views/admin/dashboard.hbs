    <div id="tab5" class="tab-content">

        <!--  Custom breadcrumb  -->

        <div class="content ">
            <div>
                <ul class="bar">
                    <li class="bar-item"><a href="/admin">Dashboard</a></li>
                    <span>/</span>
                    <li class="bar-item"><a href="/admin/view-doctors">Doctors</a></li>
                    <span>/</span>
                    <li class="bar-item active">Add</li>
                </ul>
            </div>
        </div>

        <!-- - Form  -->

        <div class="content">
            <div class="row">
                <div class="col-12 col-md-6 mb-3">
                    <div class="card">
                        <div class="card-head">
                            <div>Add Details</div>
                        </div>
                    </div>
                    <div class="card2 card">
                        <form action="/admin/add-dcotor" method="POST" enctype="multipart/form-data">
                            <div class="row">
                                <div class="form-group col-12 col-md-6">
                                    <label for="fname">First Name</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">Dr.</span>
                                        </div>
                                        <input type="text" name="fname" class="form-control side-rectangle"
                                            placeholder="John" aria-label="Username" aria-describedby="basic-addon1"
                                            required>
                                    </div>
                                </div>
                                <div class="form-group col-12 col-md-6">
                                    <label for="lname">Last Name</label>
                                    <input type="name" class="form-control" name="lname" id="lname" placeholder="Doe"
                                        required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email">Email address</label>
                                <input type="email" name="email" class="form-control" id="email"
                                    placeholder="johndoe@gmail.com" required>
                            </div>
                            <div class="form-group">
                                <label for="pwd">Password</label>
                                <input type="password" class="form-control" name="password" id="pwd"
                                    placeholder="******" required>
                            </div>
                            <div class="form-group">
                                <label for="place">Place</label>
                                <input type="text" class="form-control" name="place" id="place"
                                    placeholder="Calicut, India" required>
                            </div>
                            <div class="row ">
                                <div class="form-group col-12 col-md-6">
                                    <label for="phone">Phone Number</label>
                                    <input type="phone" class="form-control" name="phone" id="phone" placeholder="+91 "
                                        required>
                                </div>
                                <div class="col-12 col-md-6 ">
                                    <label>Gender</label>
                                    <div class="radio-box formInputs pl-2">
                                        <div class="custom-control custom-radio custom-control-inline">
                                            <input type="radio" class="custom-control-input" name="gender" value="male"
                                                id="male">
                                            <label class="custom-control-label" for="male" required>Male</label>
                                        </div>
                                        <div class="custom-control custom-radio custom-control-inline">
                                            <input type="radio" class="custom-control-input" name="gender"
                                                value="female" id="female">
                                            <label class="custom-control-label" for="female">Female</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                    </div>
                </div>
                <div class="col-12 col-md-6">
                    <div class="card">
                    </div>
                    <div class="card2 card no-border">
                        <div class="row">
                            <div class="form-group col-12 col-md-6">
                                <label for="speciality">Speciality</label>
                                <select class="form-control" name="speciality" required>
                                    {{#each specialities}}
                                    <option>{{this.speciality}}</option>
                                    {{/each}}
                                </select>
                            </div>
                            <div class="form-group col-12 col-md-6">
                                <label for="specialized">Specialized</label>
                                <input type="text" class="form-control" name="specialized" placeholder="BDS" required>
                            </div>
                        </div>
                        <div class="row formInputs">
                            <div class="form-group col-12">
                                <label for="image" class="custom-file-upload">
                                    <div class="formInputs">
                                        <i class='bx bx-cloud-upload'></i>
                                        Upload Profile Image
                                        <input type="file" class="image" id="image">
                                    </div>
                                    <img src="#" class="displayImg" name="image" alt="">
                                </label>
                            </div>
                            <div class="col-12 col-md-6 mb-3">
                                <button type="submit" class="btn btn-green">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <p></p>
            <p></p>
            {{!-- Input hidden for sending image base64 data --}}
            <input type="text" class="form-control image-label" style="visibility: hidden; position:absolute;"
                name="image" style="">
            </form>
        </div>
        <div class="modal-bg">
            <div class="modal-box col-11 col-sm-8 col-md-6 col-lg-4">
                <i class="fa fa-times-circle-o close-icon" aria-hidden="true"></i>
                <div class="col-12 image_demo" id="image_demo_add"></div>
                <a class="btn btn-green formInputs col-12 crop-btn">Crop Image</a>
            </div>
        </div>
    </div>



